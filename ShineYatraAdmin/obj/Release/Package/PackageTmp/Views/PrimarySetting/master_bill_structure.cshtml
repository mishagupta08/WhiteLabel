@using ShineYatraAdmin.Properties;
@model ShineYatraAdmin.Entity.PrimarySetting
@{
    ViewBag.Title = "master_bill_structure";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<div class="row">
    <div class="col-md-12">
        <div class="white-box">
            <h3 class="box-title m-b-0">Bill Master Structure %</h3>
            <div class="table-responsive">
                <div>
                    <div style="float: right;">
                        <div class="form-group" style="float: left;">
                            <label>Service Groups </label>
                            <select class="form-control" id="bill_service" onchange="changeBillService()">
                                <option value="">--Please Select--</option>
                                @foreach (var service in Model.bill_services)
                                {
                                    <option value="@service.sub_category">@service.sub_category</option>
                                }
                            </select>
                        </div>
                    </div>
                </div>
            </div>
                <div id="billServiceStructure">
                     
                </div>
            
        </div>
    </div>
</div>



<script src="~/plugins/bower_components/jquery/dist/jquery.min.js"></script>
<script src="~/Script/PrimarySetting.js"></script>