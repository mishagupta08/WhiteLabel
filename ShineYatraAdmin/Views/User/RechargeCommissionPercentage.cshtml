@model ShineYatraAdmin.Entity.CompanyCommissionSetting

@Html.HiddenFor(m => m.cmp_setting_id)
@Html.HiddenFor(m => m.company_id)
<div class="form-group">
    <label class="control-label col-md-3">PrePaid %</label>
    <div class="col-md-9">
        @Html.TextBoxFor(m => m.prepaid_margin, new { @required = "", @id = "prepaid_percentage", @min = "0", @class = "form-control", @type = "number" })
        <span id="prepaidperError"></span>
    </div>
</div>


<div class="form-group">
    <label class="control-label col-md-3">PostPaid %</label>
    <div class="col-md-9">
        @Html.TextBoxFor(m => m.postpaid_margin, new { @required = "", @id = "postpaid_percentage", @min = "0", @class = "form-control", @type = "number" })
        <span id="postpaidperError"></span>
    </div>
</div>


<div class="form-group">
    <label class="control-label col-md-3">DTH %</label>
    <div class="col-md-9">
        @Html.TextBoxFor(m => m.dth_margin, new { @required = "", @id = "dth_percentage", @min = "0", @class = "form-control", @type = "number" })
        <span id="dthperError"></span>
    </div>
</div>

<div class="form-actions">
    <div class="row">
        <div class="col-md-12">
            <div class="row">
                <div class="col-md-offset-3 col-md-9">
                    <button type="submit" class="btn btn-success"> <i class="fa fa-check"></i> Submit</button>
                    <button type="button" onclick="closePopup()" class="btn btn-default">Cancel</button>
                </div>
            </div>
        </div>
    </div>
</div>
