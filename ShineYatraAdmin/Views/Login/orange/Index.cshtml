@model ShineYatraAdmin.Entity.LoginModel
@{
    Layout = null;
    bool askOTP = Convert.ToString(Session["otp_login_enabled"]).ToUpper() == "Y";
}

<html>
<head>
    <title> Biz Tadka | India's #1 Multi Utility Portal </title>

    <meta content="text/html;charset=utf-8" http-equiv="Content-Type">
    <meta name="keywords" content="Biz Tadka | India's #1 Multi Utility Portal" />
    <meta name="description" content="Biz Tadka | India's #1 Multi Utility Portal">
