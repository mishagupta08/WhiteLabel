@using ShineYatraAdmin.Properties;
@model ShineYatraAdmin.Entity.CompanyViewModel
<link href="~/plugins/bower_components/sweetalert/sweetalert.css" rel="stylesheet" type="text/css">
<!-- Custom Theme JavaScript -->
<script src="~/js/cbpFWTabs.js"></script>
<script type="text/javascript">
    (function () {
        [].slice.call(document.querySelectorAll('.sttabs')).forEach(function (el) {
            new CBPFWTabs(el);
        });

    })();
</script>

<section>
    <div class="sttabs tabs-style-line">
        <h3 class="box-title m-b-0" style="margin-left:7px;">@Model.CompanyName</h3>
    <nav>
        <ul>
            <li><a href="#section-line-1"><span>@Resources.Api</span></a></li>
            <li><a href="#section-line-2"><span>@Resources.Sms</span></a></li>
            <li><a href="#section-line-3"><span>@Resources.Email</span></a></li>
            <li><a href="#section-line-4"><span>@Resources.Otp</span></a></li>
            <li><a href="#section-line-5"><span>@Resources.Commissionpercent</span></a></li>
        </ul>
    </nav>
    <div class="content-wrap">
        <section id="section-line-1">
            <div class="row">
                <div class="col-md-12">
                    <div class="panel panel-info">
                        <div class="panel-wrapper collapse in" aria-expanded="true">
                            <div class="panel-body">
                                <form action="#" class="form-horizontal form-bordered" id="@Resources.Api" onsubmit="return SubmitCompanySetting('@Resources.Api');">
                                    @Html.HiddenFor(m => m.ApiSetting.cmp_setting_id)
                                    @Html.HiddenFor(m => m.ApiSetting.company_id)
                                    <div class="form-body">
                                        <div class="form-group">
                                            <label class="control-label col-md-3">Web Login Api Enabled </label>
                                            <div class="col-md-9">
                                                @Html.DropDownListFor(m => m.ApiSetting.web_login_api_enabled, new SelectList(Model.SettingList, "Id", "Value"), new { @required = "0", id = "loginApiDropDown", @class = "form-control" })
                                                @*<select class="form-control">
                                                        <option value="Y">Yes</option>
                                                        <option value="N">No</option>
                                                    </select>*@
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="control-label col-md-3">Web Ewallet Api Enabled </label>
                                            <div class="col-md-9">
                                                @Html.DropDownListFor(m => m.ApiSetting.web_ewallet_api_enabled, new SelectList(Model.SettingList, "Id", "Value"), new { @required = "0", id = "loginApiDropDown", @class = "form-control" })
                                                @*<select class="form-control">
                                                        <option value="Y">Yes</option>
                                                        <option value="N">No</option>
                                                    </select>*@
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="control-label col-md-3">Web Payment Gateway Api Enabled </label>
                                            <div class="col-md-9">
                                                @Html.DropDownListFor(m => m.ApiSetting.web_pg_api_enabled, new SelectList(Model.SettingList, "Id", "Value"), new { @required = "0", id = "loginApiDropDown", @class = "form-control" })
                                                @*<select class="form-control">
                                                        <option value="Y">Yes</option>
                                                        <option value="N">No</option>
                                                    </select>*@
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="control-label col-md-3">APP Login Api Enabled </label>
                                            <div class="col-md-9">
                                                @Html.DropDownListFor(m => m.ApiSetting.app_login_api_enabled, new SelectList(Model.SettingList, "Id", "Value"), new { @required = "0", id = "loginApiDropDown", @class = "form-control" })
                                                @*<select class="form-control">
                                                        <option value="Y">Yes</option>
                                                        <option value="N">No</option>
                                                    </select>*@
                                            </div>
                                        </div>


                                        <div class="form-group">
                                            <label class="control-label col-md-3">APP Ewallet Api Enabled </label>
                                            <div class="col-md-9">
                                                @Html.DropDownListFor(m => m.ApiSetting.app_ewallet_api_enabled, new SelectList(Model.SettingList, "Id", "Value"), new { @required = "0", id = "loginApiDropDown", @class = "form-control" })
                                                @*<select class="form-control">
                                                        <option value="Y">Yes</option>
                                                        <option value="N">No</option>
                                                    </select>*@
                                            </div>
                                        </div>


                                        <div class="form-group">
                                            <label class="control-label col-md-3">APP Payment Gateway Api Enabled </label>
                                            <div class="col-md-9">
                                                @Html.DropDownListFor(m => m.ApiSetting.app_pg_api_enabled, new SelectList(Model.SettingList, "Id", "Value"), new { @required = "0", id = "loginApiDropDown", @class = "form-control" })
                                                @*<select class="form-control">
                                                        <option value="Y">Yes</option>
                                                        <option value="N">No</option>
                                                    </select>*@
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-actions">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="row">
                                                    <div class="col-md-offset-3 col-md-9">
                                                        <button type="submit" class="btn btn-success"> <i class="fa fa-check"></i> Submit</button>
                                                        <button type="reset" onclick="closePopup()" class="btn btn-default">Cancel</button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <section id="section-line-2">

            <div class="row">
                <div class="col-md-12">
                    <div class="panel panel-info">
                        <div class="panel-wrapper collapse in" aria-expanded="true">
                            <div class="panel-body">
                                <form action="#" class="form-horizontal form-bordered" id="@Resources.Sms" onsubmit="return SubmitCompanySetting('@Resources.Sms');">
                                    @Html.HiddenFor(m => m.SmsSetting.cmp_setting_id)
                                    @Html.HiddenFor(m => m.SmsSetting.company_id)
                                    <div class="form-body">
                                        <div class="form-group">
                                            <label class="control-label col-md-3">SMS URL</label>
                                            <div class="col-md-9">
                                                @*<input type="text" class="form-control">*@
                                                @Html.TextBoxFor(m => m.SmsSetting.sms_api_url, new { @required = "", @id = "smsUrl", @class = "form-control" })
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="control-label col-md-3">User Name</label>
                                            <div class="col-md-9">
                                                @*<input type="text" class="form-control">*@
                                                @Html.TextBoxFor(m => m.SmsSetting.sms_api_username, new { @required = "", @id = "smsUsername", @class = "form-control" })
                                            </div>
                                        </div>


                                        <div class="form-group">
                                            <label class="control-label col-md-3">Password</label>
                                            <div class="col-md-9">
                                                @*<input type="text" class="form-control">*@
                                                @Html.TextBoxFor(m => m.SmsSetting.sms_api_password, new { @required = "", @type = "password", @id = "smsPassword", @class = "form-control" })
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="control-label col-md-3">SenderID</label>
                                            <div class="col-md-9">
                                                @*<input type="text" class="form-control">*@
                                                @Html.TextBoxFor(m => m.SmsSetting.sms_api_sender_id, new { @required = "", @id = "smsSender", @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-actions">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="row">
                                                    <div class="col-md-offset-3 col-md-9">
                                                        <button type="submit" class="btn btn-success"> <i class="fa fa-check"></i> Submit</button>
                                                        <button type="reset" onclick="closePopup()" class="btn btn-default">Cancel</button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </section>
