                                        <div class="form-group">
                                            <label class="control-label col-md-3">Login OTP Enabled</label>
                                            <div class="col-md-9">
                                                @Html.DropDownListFor(m => m.OtpSetting.otp_login_enabled, new SelectList(Model.SettingList, "Id", "Value"), new { @required = "0", id = "otpApiDropDown", @class = "form-control" })
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="control-label col-md-3">Services OTP Enabled</label>
                                            <div class="col-md-9">
                                                @Html.DropDownListFor(m => m.OtpSetting.otp_login_service, new SelectList(Model.SettingList, "Id", "Value"), new { @required = "0", id = "otpApiDropDown", @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-actions">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="row">
                                                    <div class="col-md-offset-3 col-md-9">
                                                        <button type="submit" class="btn btn-success"> <i class="fa fa-check"></i> Submit</button>
                                                        <button type="reset" onclick="closePopup()" class="btn btn-default">Cancel</button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </section>
        <section id="section-line-5">            
                <div class="row">
                    <div class="col-md-12">
                        <div class="panel panel-info">
                            <div class="panel-wrapper collapse in" aria-expanded="true">
                                <div class="panel-body">
                                    <form class="form-horizontal form-bordered" id="commissionpercent" onsubmit="return SubmitCompanySetting('commissionpercent');">
                                    @Html.HiddenFor(m => m.CommissionSetting.cmp_setting_id)
                                    @Html.HiddenFor(m => m.CommissionSetting.company_id)
                                            <div class="form-group">
                                                <label class="control-label col-md-3">PrePaid %</label>
                                                <div class="col-md-9">
                                                    @Html.TextBoxFor(m => m.CommissionSetting.prepaid_margin, new { @required = "", @id = "prepaid_percentage",@min="0", @class = "form-control", @type="number" })
                                                    <span id="prepaidperError"></span>
                                                </div>
                                            </div>


                                            <div class="form-group">
                                                <label class="control-label col-md-3">PostPaid %</label>
                                                <div class="col-md-9">
                                                    @Html.TextBoxFor(m => m.CommissionSetting.postpaid_margin, new { @required = "", @id = "postpaid_percentage", @min = "0", @class = "form-control", @type = "number" })
                                                    <span id="postpaidperError"></span>
                                                </div>
                                            </div>


                                            <div class="form-group">
                                                <label class="control-label col-md-3">DTH %</label>
                                                <div class="col-md-9">
                                                    @Html.TextBoxFor(m => m.CommissionSetting.dth_margin, new { @required = "", @id = "dth_percentage", @min = "0", @class = "form-control", @type = "number" })
                                                    <span id="dthperError"></span>
                                                </div>
                                            </div>
                                       
                                        <div class="form-actions">
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="row">
                                                        <div class="col-md-offset-3 col-md-9">
                                                            <button type="submit" class="btn btn-success"> <i class="fa fa-check"></i> Submit</button>
                                                            <button type="button" onclick="closePopup()" class="btn btn-default">Cancel</button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>            


